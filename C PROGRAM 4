#include<stdio.h>
int main() {
    float length, breadth, area, perimeter;
    printf("enter length and breadth :");
    scanf("%f %f", &length, &breadth);
    area = length * breadth;
    perimeter = 2 * (length + breadth);
    printf("area = %.2f\n perimeter = %.2f", area, perimeter);
    return 0;
}